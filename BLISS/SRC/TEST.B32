MODULE EXAMPLE (MAIN = STARTUP) =
BEGIN

!++
!
! Function description:
!
!	This routine displays a single 'A' character to the
!	available text mode video graphics screen terminal.
!
! Inputs:
!
!	None.
!
! Outputs:
!
!	None.
!
!--

ROUTINE DISPLCHAR =
BEGIN

	!
	! Address 0x400 contains the screen buffer. We write 0x41
	! (ASCII for 'A') to it directly.
	!
	
    %X'400' = %X'41';
    
    RETURN 1;
END;

ROUTINE STARTUP =
BEGIN
	!
	! Call the routine and exit. 
	!
	
    DISPLCHAR();
    
    RETURN 1;
END;

END
ELUDOM
